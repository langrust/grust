rust_defaults {
    name: "json_defaults",
    defaults: ["sdv@rust_defaults"],
    rustlibs: [
        "libserde",
        "libserde_json",
    ],
    vendor_available: true,
    product_available: true,
    host_supported: true,
}

rust_library {
    name: "libjson",
    srcs: ["src/lib.rs"],
    defaults: ["json_defaults"],
    crate_name: "json",
}