rust_defaults {
    name: "timer_stream_defaults",
    defaults: ["sdv@rust_defaults"],
    rustlibs: [
        "libpin_project",
        "librand",
        "libtokio",
        "libfutures",
    ],
    vendor_available: true,
    product_available: true,
    host_supported: true,
    proc_macros: [
        "libpin_project",
    ],
}

rust_library {
    name: "libtimer_stream",
    srcs: ["src/lib.rs"],
    defaults: ["timer_stream_defaults"],
    crate_name: "timer_stream",
}