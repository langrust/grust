rust_defaults {
    name: "grust_defaults",
    defaults: ["sdv@rust_defaults"],
    rustlibs: [
        "libgrust_std",
        "libgrust_core",
    ],
    vendor_available: true,
    product_available: true,
    host_supported: true,
    proc_macros: [
        "libgrust_proc_macros",
    ],
}

rust_library {
    name: "libgrust",
    srcs: ["src/lib.rs"],
    defaults: ["grust_defaults"],
    crate_name: "grust",
}