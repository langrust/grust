rust_defaults {
    name: "compiler_top_defaults",
    defaults: ["sdv@rust_defaults"],
    rustlibs: [
        "libcompiler_common",
        "libcompiler_ir0",
        "libcompiler_ir1",
        "libcompiler_ir2",        
    ],
    vendor_available: true,
    product_available: true,
    host_supported: true,
}

rust_library {
    name: "libcompiler_top",
    srcs: ["src/lib.rs"],
    defaults: ["compiler_top_defaults"],
    crate_name: "compiler_top",
    features: ["no_diagnostics"],
}